# Docker Production Environment Variables

# Node Environment
NODE_ENV=production

# Server
PORT=3000

# MongoDB
MONGODB_URI=mongodb://mongo:27017/advenro

# JWT
JWT_SECRET=your_jwt_secret_here  # This will be replaced by deploy.sh

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX=100

# Cors
CORS_ORIGIN=*

# File Upload
MAX_FILE_SIZE=5242880  # 5MB 