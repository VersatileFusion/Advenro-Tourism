{
  "name": "tourism-api",
  "version": "1.0.0",
  "description": "Tourism booking API with user profiles, reviews, and notifications",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "k6 run tests/performance/**/*.js",
    "test:api-docs": "jest src/tests/api-docs.test.js",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e && npm run test:api-docs",
    "create-admin": "node src/scripts/createAdmin.js"
  },
  "keywords": [
    "tourism",
    "booking",
    "pwa",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.0.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.0",
    "qrcode": "^1.5.3",
    "sharp": "^0.32.1",
    "speakeasy": "^2.0.0",
    "stripe": "^17.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-parser": "^10.0.3",
    "swagger-ui-express": "^4.6.3",
    "twilio": "^4.11.0",
    "web-push": "^3.6.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "ajv": "^8.12.0",
    "cypress": "^13.6.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^15.0.0",
    "k6": "^0.0.0",
    "mongodb-memory-server": "^8.12.2",
    "msw": "^2.2.2",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.4",
    "swagger-parser": "^10.0.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./src/tests/setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/tests/**",
      "!src/scripts/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "verbose": true
  }
}
